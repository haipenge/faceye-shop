<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN" "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">
<tiles-definitions>
    <!-- 模块:product 业务对像:DataType 管理后台 tiles模板配置 -->
    <!-- 作者:haipege -->
    <!-- DataType 管理 首页(列表页) -->
	<definition name="product.dataType.manager" extends="default.manager">
	    <put-attribute name="default-center" value="product.dataType.center"></put-attribute>
	</definition>
	<definition name="product.dataType.center" extends="default.center">
	    <put-attribute name="default-center-right" value="/component/product/dataType/dataType_center.jsp"></put-attribute>
	</definition>
	
	<!-- DataType 新增,编辑页面 -->
	<definition name="product.dataType.update" extends="default.manager">
	    <put-attribute name="default-center" value="product.dataType.update.center"></put-attribute>
	</definition>
	<definition name="product.dataType.update.center" extends="default.center">
	    <put-attribute name="default-center-right" value="/component/product/dataType/dataType_update.jsp"></put-attribute>
	</definition>
	
	<!-- DataType 明细页 -->
	<definition name="product.dataType.detail" extends="default.manager">
	    <put-attribute name="default-center" value="product.dataType.detail.center"></put-attribute>
	</definition>
	<definition name="product.dataType.detail.center" extends="default.center">
	    <put-attribute name="default-center-right" value="/component/product/dataType/dataType_detail.jsp"></put-attribute>
	</definition>
	
</tiles-definitions>
